# ===============================
# Angular / Node / Typescript Gitignore
# ===============================

# Angular build outputs
dist/
build/
out-tsc/

# Angular cache
.angular/

# Node dependencies
node_modules/

# IDE / Editor
.vscode/
.idea/
.DS_Store

# Logs
npm-debug.log*
yarn-debug.log*
yarn-error.log*
pnpm-debug.log*

# Environment files (do NOT commit secrets)
src/environments/environment.ts
src/environments/environment.prod.ts

# OS generated
Thumbs.db

# Compiled output
*.js
*.js.map

# Temporary files
*.temp
*.tmp

# Allow Docs
!docs/
